{
  "name": "@metztheolab/xml-editor-library",
  "version": "0.1.7",
  "description": "XML Editor Components Library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/xml-editor-library/xml-editor-library.esm.js",
  "exports": {
    ".": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    },
    "./components/xec-annotation-form": {
      "import": "./dist/components/xec-annotation-form.js",
      "types": "./dist/components/xec-annotation-form.d.ts"
    },
    "./components/xec-blank-space-form": {
      "import": "./dist/components/xec-blank-space-form.js",
      "types": "./dist/components/xec-blank-space-form.d.ts"
    },
    "./components/xec-button": {
      "import": "./dist/components/xec-button.js",
      "types": "./dist/components/xec-button.d.ts"
    },
    "./components/xec-dropdown": {
      "import": "./dist/components/xec-dropdown.js",
      "types": "./dist/components/xec-dropdown.d.ts"
    },
    "./components/xec-editor": {
      "import": "./dist/components/xec-editor.js",
      "types": "./dist/components/xec-editor.d.ts"
    },
    "./components/xec-icon": {
      "import": "./dist/components/xec-icon.js",
      "types": "./dist/components/xec-icon.d.ts"
    },
    "./components/xec-popup": {
      "import": "./dist/components/xec-popup.js",
      "types": "./dist/components/xec-popup.d.ts"
    },
    "./components/xec-select": {
      "import": "./dist/components/xec-select.js",
      "types": "./dist/components/xec-select.d.ts"
    },
    "./components/xec-settings-form": {
      "import": "./dist/components/xec-settings-form.js",
      "types": "./dist/components/xec-settings-form.d.ts"
    },
    "./components/xec-structure-form": {
      "import": "./dist/components/xec-structure-form.js",
      "types": "./dist/components/xec-structure-form.d.ts"
    },
    "./components/xec-textfield": {
      "import": "./dist/components/xec-textfield.js",
      "types": "./dist/components/xec-textfield.d.ts"
    },
    "./components/xec-toolbar": {
      "import": "./dist/components/xec-toolbar.js",
      "types": "./dist/components/xec-toolbar.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scribes-app/xml-editor-cmp.git"
  },
  "files": [
    "dist/",
    "dist/components/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/angular-output-target": "^0.9.1",
    "@stencil/core": "^4.22.1",
    "@stencil/react-output-target": "^0.7.3",
    "@stencil/sass": "^3.0.12",
    "@stencil/vue-output-target": "^0.9.0",
    "@types/html-escaper": "^3.0.2",
    "classnames": "^2.5.1",
    "parchment": "^3.0.0",
    "quill": "^2.0.2",
    "xml-formatter": "^3.6.3"
  }
}
